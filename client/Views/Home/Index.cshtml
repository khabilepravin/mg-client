@{
    ViewData["Title"] = "MovieGrep-Home of movie quotes";
}
<div class="list-group list-group-bordered mb-3">
    <a href="#!" class="list-group-item list-group-item-action" v-for="item in items">
        <div class="list-group-item-figure">
            <div class="tile bg-success">
                <span class="fas fa-film"></span>
            </div>
        </div>
        <div class="list-group-item-body">{{ item.name }}</div>
    </a> 
</div>